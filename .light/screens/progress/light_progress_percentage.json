{
  "namespace": "light_progress_percentage",
  "percentage_bindings": {
    "size": [
      "100%",
      "100%"
    ],
    "$total": "#loading_bar_total_amount",
    "$actual": "#loading_bar_current_amount",
    "bindings": [
      {
        "binding_name": "$total"
      },
      {
        "binding_name": "$actual"
      },
      {
        "binding_type": "view",
        "source_property_name": "((($actual / $total) > $min) and (($actual / $total) < $max))",
        "target_property_name": "#visible"
      }
    ]
  },
  "percent_label@percentage_bindings": {
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "type": "stack_panel",
    "layer": 100,
    "orientation": "horizontal",
    "size": [
      12,
      12
    ],
    "$number_text|default": "",
    "controls": [
      {
        "number": {
          "type": "label",
          "text": "$number_text",
          "color": "white",
          "shadow": true,
          "layer": 100,
          "localize": false,
          "size": [
            "default",
            12
          ]
        }
      },
      {
        "percentage": {
          "type": "label",
          "text": "%",
          "color": "white",
          "shadow": true,
          "font_scale_factor": 1.2,
          "layer": 100,
          "localize": false,
          "size": [
            "default",
            12
          ]
        }
      }
    ]
  },
